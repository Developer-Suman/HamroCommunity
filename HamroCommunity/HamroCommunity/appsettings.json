{
  "Serilog": {
    //"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    //"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "log/SumanLog-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",


          "restrictedToMinimumLevel": "Information"
        }

      },

      {
        "Name": "File",
        "Args": {
          "path": "log/log.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:8081"
        }
      }
    ]
    //"Filter": [
    //  {
    //    "Name": "ByIncludingOnly",
    //    "Args": {
    //      "expression": "Contains(@Message, 'Request starting') || Contains(@Message, 'Request finished')"
    //    }
    //  }
    //]
  },
  "ConnectionStrings": {
    //"DefaultConnection": "Server=SUMANEY; Database=HamroCommunity; TrustServerCertificate=True; Trusted_Connection=True"
    "DefaultConnection": "Server=SUMANEY;Database=HamroCommunity;TrustServerCertificate=True;User Id=sa;Password=YourStrong!Passw0rd;"
  },

  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "HamroCommunityDevelopedBySuman",
    "Audience": "HamroCommunityDevelopedBySuman",
    "Key": "HamroCommunityDevelopedBySumanAndKailash",
    "TokenValidityInMinutes": 120,
    "RefreshTokenValidityInDays": 7
  }

}
